package Charactor_Programs;

import java.util.Scanner;

public class Replace 
{
   public static void main(String[] args) {
	
	   String word="";
	   String res="";
	   
	   Scanner sc=new Scanner(System.in);
	   System.out.println(" enter Sstring ");
	   String s=sc.next();
	    s=s+" ";
//	   String s="school i am going to school ";
	   
	   for(int i=0;i<s.length();i++)
	   {
		   char ch=s.charAt(i);
           
		   if(ch!=' ')
			   word+=ch;
		   else
		   {
			  String rep= "school";
			   if(word.equals(rep))
				   word="maribasava";
			   
			   res+=word+" ";
			   
			   word="";

		   }
			   
	   }
	   System.out.println(res);
}
}
